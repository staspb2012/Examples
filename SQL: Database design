CREATE TABLE teachers (
teacher_no CHAR(4) NOT NULL PRIMARY KEY,
teacher_name VARCHAR(30) NOT NULL,
phone_no VARCHAR(15) NOT NULL);

CREATE UNIQUE INDEX tphone ON teachers(phone_no);

