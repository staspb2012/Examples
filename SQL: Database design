EXPLAIN SELECT * -- 3
FROM students
WHERE birth_date BETWEEN '1989-01-01' AND '1989-12-31'; -- '1', 'SIMPLE', 'students', 'p5', 'ALL', NULL, NULL, NULL, NULL, '1', '100.00', 'Using where'

EXPLAIN SELECT * -- 4
FROM teachers
WHERE phone_no = '099456432';
